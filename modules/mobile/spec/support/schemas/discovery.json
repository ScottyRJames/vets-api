{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "type": "object",
      "required": [
        "id",
        "type",
        "attributes"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "attributes": {
          "type": "object",
          "required": [
            "authBaseUrl",
            "apiRootUrl",
            "minimumVersion",
            "maintenanceWindows",
            "webViews"
          ],
          "properties": {
            "authBaseUrl": {
              "type": "string"
            },
            "apiRootUrl": {
              "type": "string"
            },
            "minimumVersion": {
              "type": "string"
            },
            "maintenanceWindows": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "service",
                  "startTime",
                  "endTime",
                  "description"
                ],
                "properties": {
                  "service": {
                    "type": "string"
                  },
                  "startTime": {
                    "type": "string"
                  },
                  "endTime": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                }
              }
            },
            "web_views": {
              "type": "object",
              "required": [
                "coronaFaq",
                "facilityLocator"
              ],
              "properties": {
                "coronaFaq": {
                  "type": "string"
                },
                "facilityLocator": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
